{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml",
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "brand": "advocates",
      "uhfHeaderId": "MSDocsHeader-Advocates"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "cloud-developer-advocates",
    "exportRawModel": true,
    "rawModelOutputFolder": "../_model",
    "exportViewModel": true,
    "viewModelOutputFolder": "../_viewmodel",
    "markdownEngineName": "markdig"
  }
}
